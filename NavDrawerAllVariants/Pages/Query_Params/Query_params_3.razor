@layout Query_Params
@page "/query-params/query-params-3"

@using System.Web
@inject NavigationManager NavigationManager

<div class="row-layout query-params-3-container">
    <h5 class="h5">
        QUERY PARAMS 3
    </h5>
</div>

@code {
    [Parameter]
    [SupplyParameterFromQuery]
    public double Q22 { get; set; }
    [Parameter]
    [SupplyParameterFromQuery]
    public double Q12 { get; set; }

    protected override void OnParametersSet()
    {
        var uri = new Uri(NavigationManager.Uri);
        var queryParams = HttpUtility.ParseQueryString(uri.Query);

        var q22 = queryParams.Get("q22");
        Q22 = string.IsNullOrEmpty(q22) ? 302 : Convert.ToDouble(q22);

        var q12 = queryParams.Get("q12");
        Q12 = string.IsNullOrEmpty(q12) ? 301 : Convert.ToDouble(q12);

    }
}
